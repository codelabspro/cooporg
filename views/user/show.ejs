<div class="container">
  <h1><%= user.username %></h1>
  <% if (user.admin) { %>
    <td><span class="glyphicon glyphicon-plus" aria-hidden="true"></span></td>
  <% } else { %>
      <td><span class="glyphicon glyphicon-asterisk" aria-hidden="true"></span></td>
  <% } %>
  <h3><%= user.title %></h3>
  <hr>
  <h3>contact: <%- user.email %></h3>
  <a class="btn btn-medium btn-primary" href="/user/edit/<%= user.id %>">Edit</a>
  <a href="/session/destroy" class="btn btn-medium btn-success">Sign out</a>
</div>
